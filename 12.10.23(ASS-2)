sno = int(input('Enter student number : '))
sname = (input('Enter sstudent name : '))
group = (input('Enter student group : '))
s1 = int(input('Enter s1 marks : '))
s2 = int(input('Enter s2 marks : '))
s3 = int(input('Enter s3 marks : '))
total = s1 + s2 + s3
avg = total//3
if(avg>=90):
    print('O-grade')
elif(avg>=80):
    print('A-grade')
elif(avg>=70):
    print('B-grade')
elif(avg>=60):
    print('C-grade')
elif(avg>=50):
    print('D-grade')
else:
    print('pass')
